package cornclicker;
import javax.swing.JFrame;

public class CornClickerRun {
    public static void main(String[] args)
    {
        MetroSound sound = new MetroSound("CornTheme.wav");
        CornClicker buttonFrame = new CornClicker();
        buttonFrame.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        buttonFrame.setSize(440,550);
        buttonFrame.setResizable(false);
        buttonFrame.setLocationRelativeTo(null);
        buttonFrame.setVisible(true);
    }//end main
    
}//end class CornClickerRun
